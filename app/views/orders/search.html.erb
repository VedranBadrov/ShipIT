<h1>Track your order</h1>
<%= form_tag(search_path, method: "get") do %>
  <%= label_tag(:order_number, "Enter Tracking Number:") %>
  <%= text_field_tag(:order_number) %>
  <%= submit_tag("Search") %>
<% end %>

<% if flash[:alert] %>
  <div class="alert alert-danger"><%= flash[:alert] %></div>
<% end %>